<?xml version="1.0"?>
<!--Test-specific configuration for 7-Zip Compression 1.9.0 benchmark-->
<!--7-Zip auto-detects CPU count; no test options required-->
<PhoronixTestSuite>
  <Options>
    <Testing>
      <SaveTestLogs>TRUE</SaveTestLogs>
    </Testing>
    <TestResultValidation>
      <DynamicRunCount>FALSE</DynamicRunCount>
      <LimitDynamicToTestLength>20</LimitDynamicToTestLength>
    </TestResultValidation>
  </Options>
</PhoronixTestSuite>
