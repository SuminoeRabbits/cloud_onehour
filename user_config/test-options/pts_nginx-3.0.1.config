<?xml version="1.0"?>
<!--Test-specific configuration for nginx 3.0.1 benchmark-->
<!--Multi-threaded: wrk client uses NUM_CPU_CORES, nginx server uses worker_processes auto (custom install.sh)-->
<PhoronixTestSuite>
  <Options>
    <Testing>
      <SaveTestLogs>TRUE</SaveTestLogs>
    </Testing>
    <TestResultValidation>
      <DynamicRunCount>FALSE</DynamicRunCount>
      <LimitDynamicToTestLength>20</LimitDynamicToTestLength>
    </TestResultValidation>
  </Options>
  <TestOptions>
    <Test>
      <Identifier>pts/nginx-3.0.1</Identifier>
      <Option>100</Option>
    </Test>
  </TestOptions>
</PhoronixTestSuite>
