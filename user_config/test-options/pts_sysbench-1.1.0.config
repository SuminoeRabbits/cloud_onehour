<?xml version="1.0"?>
<!--Test-specific configuration for Sysbench 1.1.0 benchmark-->
<!--Supports AVX-512 via CFLAGS, Multi-threaded-->
<PhoronixTestSuite>
  <Options>
    <Testing>
      <SaveTestLogs>TRUE</SaveTestLogs>
    </Testing>
    <TestResultValidation>
      <DynamicRunCount>FALSE</DynamicRunCount>
      <LimitDynamicToTestLength>20</LimitDynamicToTestLength>
    </TestResultValidation>
  </Options>
  <TestOptions>
    <Test>
      <Identifier>pts/sysbench-1.1.0</Identifier>
      <Option>1</Option>
    </Test>
  </TestOptions>
</PhoronixTestSuite>
