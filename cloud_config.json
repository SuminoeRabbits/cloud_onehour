{
  "common": {
    "os_version": "24.04",
    "ssh_user": "ubuntu",
    "ssh_key_name": "cloud_onehour_project",
    "ssh_key_path": "${HOME}/.ssh/cloud_onehour_project.pem",
    "testloads": [
      "sudo apt-get -y update > /tmp/apt_update_test.log 2>&1",
      "sudo apt-get install -y git curl python3-pip > /tmp/apt_install_test.log 2>&1",
      "git clone https://github.com/SuminoeRabbits/cloud_onehour.git > /tmp/git_test.log 2>&1",
      "python3 --version && pip3 --version > /tmp/python_env_test.log 2>&1",
      "df -h > /tmp/disk_test.log 2>&1",
      "free -m > /tmp/memory_test.log 2>&1",
      "nproc > /tmp/cpu_test.log 2>&1"
    ],
    "workloads": [
      "sudo apt-get -y update > /tmp/apt_update.log 2>&1",
      "sudo apt-get -y install -y git > /tmp/apt_install.log 2>&1",
      "git clone https://github.com/SuminoeRabbits/cloud_onehour.git > /tmp/git_clone.log 2>&1",
      "cd ~/cloud_onehour/scripts && ./prepare_tools.sh > /tmp/prepare_tools.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_coremark-1.0.1.py 4 > /tmp/pts_runner_coremark.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_simdjson-2.1.0.py 1 > /tmp/pts_runner_simdjson.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_sysbench-1.1.0.py > /tmp/pts_runner_sysbench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py 288 --quick > /tmp/pts_runner_build-linux-kernel0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py 4 --quick > /tmp/pts_runner_build-linux-kernel1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-llvm-1.6.0.py 288 --quick > /tmp/pts_runner_build-llvm0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-llvm-1.6.0.py 4 --quick > /tmp/pts_runner_build-llvm1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-gcc-1.5.0.py 288 --quick > /tmp/pts_runner_build-gcc0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-gcc-1.5.0.py 4 --quick > /tmp/pts_runner_build-gcc1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-7zip-1.12.0.py > /tmp/pts_runner_compress-7zip.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-zstd-1.6.0.py --quick > /tmp/pts_runner_compress-zstd.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-xz-1.1.0.py --quick > /tmp/pts_runner_compress-xz.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-lz4-1.10.0.py 1 --quick > /tmp/pts_runner_compress-lz4.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_rustls-1.0.0.py 288 --quick > /tmp/pts_runner_rustls0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_rustls-1.0.0.py 4 --quick > /tmp/pts_runner_rustls.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_openssl-3.6.0.py 288 --quick > /tmp/pts_runner_openssl0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_openssl-3.6.0.py 4 --quick > /tmp/pts_runner_openssl1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_nginx-3.0.1.py 288 --quick > /tmp/pts_runner_nginx0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_nginx-3.0.1.py 4 --quick > /tmp/pts_runner_nginx1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_java-jmh-1.0.1.py --quick > /tmp/pts_runner_java.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_apache-3.0.0.py 1 --quick > /tmp/pts_runner_apache.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_spark-1.0.1.py 288 --quick > /tmp/pts_runner_spark.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_spark-1.0.1.py 4 --quick > /tmp/pts_runner_spark.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_phpbench-1.1.6.py 1 > /tmp/pts_runner_phpbench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_ffmpeg-7.0.1.py 288 --quick > /tmp/pts_runner_ffmpeg0.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_ffmpeg-7.0.1.py 4 --quick > /tmp/pts_runner_ffmpeg1.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_x265-1.5.0.py > /tmp/pts_runner_x265.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_redis-1.3.1.py > /tmp/pts_runner_redis.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_stream-1.3.4.py > /tmp/pts_runner_stream.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_tinymembench-1.0.2.py 1 --quick > /tmp/pts_runner_tinymembench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_memcached-1.2.0.py --quick > /tmp/pts_runner_memcached.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_cachebench-1.2.0.py > /tmp/pts_runner_cachebench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_tensorflow-lite-1.1.0.py --quick > /tmp/pts_runner_tensorflow-lite.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_numpy-1.2.1.py > /tmp/pts_runner_numpy.log 2>&1",
      "cd ~/cloud_onehour/results && ./pts_runner_postmortem.py > /tmp/pts_runner_postmortem.log 2>&1",
      "cd ~/cloud_onehour/results && ./make_one_big_json.py > /tmp/make_one_big_json.log 2>&1"
    ],
    "cloud_reports_dir": "~/cloud_onehour/results",
    "host_reports_dir": "bench_results",
    "security_group_name": "cloud-benchmarking-sg",
    "ssh_timeout": 20,
    "workload_timeout": 12000,
    "ssh_strict_host_key_checking": false,
    "debug_stdout": true,
    "long_running_indicators": [
      "pts_regression.py",
      "benchmark",
      "phoronix-test-suite",
      "pts_runner",
      "prepare_tools.sh"
    ]
  },
  "instance_definitions_file": "cloud_instances.json"
}
