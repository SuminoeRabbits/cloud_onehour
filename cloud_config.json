{
  "common": {
    "os_version": "25.04",
    "ssh_user": "ubuntu",
    "ssh_key_name": "cloud_onehour_project",
    "ssh_key_path": "${HOME}/.ssh/cloud_onehour_project.pem",
    "testloads": [
      "ls -la  > /tmp/testloads.log 2>&1"
    ],
    "workloads": [
      "sudo apt-get -y update && sudo apt-get install -y git > /tmp/apt_setup.log 2>&1",
      "git clone https://github.com/SuminoeRabbits/cloud_onehour.git > /tmp/git_clone.log 2>&1",
      "cd ~/cloud_onehour/scripts && ./prepare_tools.sh > /tmp/prepare_tools.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_coremark-1.0.1.py 288 > /tmp/pts_runner_coremark.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_sysbench-1.1.0.py > /tmp/pts_runner_sysbench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py --quick > /tmp/pts_runner_build-linux-kernel.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-llvm-1.6.0.py --quick > /tmp/pts_runner_build-llvm.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_build-gcc-1.5.0.py --quick > /tmp/pts_runner_build-gcc.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-7zip-1.12.0.py > /tmp/pts_runner_compress-7zip.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-zstd-1.6.0.py > /tmp/pts_runner_compress-zstd.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_compress-xz-1.1.0.py > /tmp/pts_runner_compress-xz.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_rustls-1.0.0.py --quick > /tmp/pts_runner_rustls.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_openssl-3.6.0.py --quick > /tmp/pts_runner_openssl.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_nginx-3.0.1.py --quick > /tmp/pts_runner_nginx.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_renaissance-1.4.0.py --quick > /tmp/pts_runner_renaissance.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_java-jmh-1.0.1.py > /tmp/pts_runner_java-jmh.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_apache-3.0.0.py 1 --quick > /tmp/pts_runner_apache.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_spark-1.0.1.py --quick > /tmp/pts_runner_spark.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_ffmpeg-7.0.1.py --quick > /tmp/pts_runner_ffmpeg.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_x265-1.5.0.py > /tmp/pts_runner_x265.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_pgbench-1.11.1.py --quick > /tmp/pts_runner_pgbench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_redis-1.3.1.py > /tmp/pts_runner_redis.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_stream-1.3.4.py > /tmp/pts_runner_stream.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_tinymembench-1.0.2.py 1 > /tmp/pts_runner_tinymembench.log 2>&1",
      "cd ~/cloud_onehour && ./pts_runner/pts_runner_pmbench-1.0.2.py --quick > /tmp/pts_runner_pmbench.log 2>&1"
    ],
    "cloud_reports_dir": "~/cloud_onehour/results",
    "host_reports_dir": "bench_results",
    "security_group_name": "cloud-benchmarking-sg",
    "ssh_timeout": 20,
    "workload_timeout": 5400,
    "ssh_strict_host_key_checking": false,
    "debug_stdout": true,
    "long_running_indicators": [
      "pts_regression.py",
      "benchmark",
      "phoronix-test-suite",
      "pts_runner",
      "prepare_tools.sh"
    ]
  },
