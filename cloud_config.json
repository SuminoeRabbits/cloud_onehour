{
  "common": {
    "os_version": "orcl10",
    "ssh_user": "opc",
    "ssh_key_name": "cloud_onehour_project",
    "ssh_key_path": "${HOME}/.ssh/cloud_onehour_project.pem",
    "debian_setup": [
      "sudo apt-get -y update > /tmp/apt_update.log 2>&1",
      "sudo apt-get -y install git curl python3 python3-pip > /tmp/apt_install.log 2>&1",
      "[ -d ~/cloud_onehour/.git ] || git clone https://github.com/SuminoeRabbits/cloud_onehour.git ~/cloud_onehour > /tmp/git_clone.log 2>&1",
      "cd ~/cloud_onehour/scripts && ./prepare_tools.sh > /tmp/prepare_tools.log 2>&1"
    ],
    "fedra_setup": [
      "sudo dnf -y update > /tmp/dnf_update.log 2>&1",
      "sudo dnf -y install git curl python3 python3-pip > /tmp/dnf_install.log 2>&1",
      "[ -d ~/cloud_onehour/.git ] || git clone https://github.com/SuminoeRabbits/cloud_onehour.git ~/cloud_onehour > /tmp/git_clone.log 2>&1",
      "cd ~/cloud_onehour/scripts_rhel9 && ./prepare_tools.sh > /tmp/prepare_tools.log 2>&1"
    ],
    "workloads": [
      "rm -rf ~/.phoronix-test-suite/installed-tests/pts/* > /tmp/pts_cache_clean.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_coremark-1.0.1.py 16 > /tmp/pts_runner_coremark.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_simdjson-2.1.0.py 1 > /tmp/pts_runner_simdjson.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_sysbench-1.1.0.py > /tmp/pts_runner_sysbench.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_compress-7zip-1.12.0.py > /tmp/pts_runner_compress-7zip.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_java-jmh-1.0.1.py > /tmp/pts_runner_java-jmh.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_phpbench-1.1.6.py 1 > /tmp/pts_runner_phpbench.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_x265-1.5.0.py > /tmp/pts_runner_x265.log 2>&1",
      "rm -rf ~/.phoronix-test-suite/installed-tests/pts/* > /tmp/pts_cache_clean.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_redis-1.3.1.py 1 > /tmp/pts_runner_redis.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_valkey-1.0.0.py 1 > /tmp/pts_runner_valkey.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_stream-1.3.4.py > /tmp/pts_runner_stream.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py 8 --quick > /tmp/pts_runner_build-linux-kernel.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py 12 --quick >> /tmp/pts_runner_build-linux-kernel.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-linux-kernel-1.17.1.py 16 --quick >> /tmp/pts_runner_build-linux-kernel.log 2>&1",
      "rm -rf ~/.phoronix-test-suite/installed-tests/pts/build-linux-kernel* > /dev/null 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-llvm-1.6.0.py 8 --quick > /tmp/pts_runner_build-llvm.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-llvm-1.6.0.py 12 --quick >> /tmp/pts_runner_build-llvm.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-llvm-1.6.0.py 16 --quick >> /tmp/pts_runner_build-llvm.log 2>&1",
      "rm -rf ~/.phoronix-test-suite/installed-tests/pts/build-llvm* > /dev/null 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-gcc-1.5.0.py 8 --quick > /tmp/pts_runner_build-gcc.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-gcc-1.5.0.py 12 --quick >> /tmp/pts_runner_build-gcc.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_build-gcc-1.5.0.py 16 --quick >> /tmp/pts_runner_build-gcc.log 2>&1",
      "rm -rf ~/.phoronix-test-suite/installed-tests/pts/build-llvm* > /dev/null 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_renaissance-1.4.0.py 8 --quick > /tmp/pts_runner_renaissance.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_renaissance-1.4.0.py 12 --quick >> /tmp/pts_runner_renaissance.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_renaissance-1.4.0.py 16 --quick >> /tmp/pts_runner_renaissance.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_pgbench-1.11.1.py 8 --quick > /tmp/pts_runner_pgbench.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_pgbench-1.11.1.py 12 --quick >> /tmp/pts_runner_pgbench.log 2>&1",
      "cd ~/cloud_onehour && git pull && ./pts_runner/pts_runner_pgbench-1.11.1.py 16 --quick >> /tmp/pts_runner_pgbench.log 2>&1",
      "cd ~/cloud_onehour/results && ./pts_runner_postmortem.py > /tmp/pts_runner_postmortem.log 2>&1",
      "cd ~/cloud_onehour/results && ./make_one_big_json.py > /tmp/make_one_big_json.log 2>&1"
    ],
    "cloud_reports_dir": "~/cloud_onehour/results",
    "host_reports_dir": "bench_results",
    "security_group_name": "cloud-benchmarking-sg",
    "ssh_timeout": 20,
    "workload_timeout": 9000,
    "workload_timeout_limit": 1,
    "stop_after_error": true,
    "ssh_strict_host_key_checking": false,
    "debug_stdout": true,
    "long_running_indicators": [
      "pts_regression.py",
      "benchmark",
      "phoronix-test-suite",
      "pts_runner"
    ]
  },
  "instance_definitions_file": "cloud_instances.json"
}